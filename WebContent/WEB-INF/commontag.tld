<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib
	PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.1//EN"
	"http://java.sun.com/j2ee/dtds/web-jsptaglibrary_1_1.dtd">

<!-- Tag Library Descriptor for formatting dates and currency -->

<taglib>
    <tlibversion>1.0</tlibversion>
    <jspversion>1.1</jspversion>
    <shortname>pds</shortname>
    <info>This tag library contains tag extentions for formatting dates and currency. </info>

    <!-- ===================== Formatter Tags ====================== -->
    <tag>
        <name>pageform</name>
        <tagclass>cn.grgbanking.phosphorframework.webapp.taglib.PageFormTag</tagclass>
        <attribute>
            <name>action</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>actionParameter</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>

        <attribute>
            <name>pageName</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>formName</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>submitJSMethodName</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>scrollPageConfirm</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>scrollPageConfirmMsg</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        
        
    </tag>
    <tag>
        <name>interactiveRow</name>
        <tagclass>cn.grgbanking.phosphorframework.webapp.taglib.InteractiveRow</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>first</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>second</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>subright</name>
        <tagclass>cn.grgbanking.phosphorframework.webapp.taglib.GetSubRightTag</tagclass>
        <bodycontent>empty</bodycontent>
        <attribute>
            <name>supRightId</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
        <attribute>
            <name>roleId</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>isRole</name>
        <tagclass>cn.grgbanking.phosphorframework.webapp.taglib.IsRole</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>
    <tag>
        <name>isNotRole</name>
        <tagclass>cn.grgbanking.phosphorframework.webapp.taglib.IsNotRole</tagclass>
