        <attribute>
            <name>value</name>
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
        </attribute>
    </tag>


    <tag>
        <name>isIn</name>
        <tagclass>cn.grgbanking.phosphorframework.webapp.taglib.ConditionInTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <attribute>
            <name>cookie</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
