       <name>NotSuperAdmin</name>
       <tagclass>cn.grgbanking.phosphorframework.webapp.taglib.NotSuperAdminTag</tagclass>
       <bodycontent>JSP</bodycontent>
    </tag>
     <tag>
       <name>IsSuperAdmin</name>
       <tagclass>cn.grgbanking.phosphorframework.webapp.taglib.IsSuperAdminTag</tagclass>
       <bodycontent>JSP</bodycontent>
    </tag>
</taglib>
