<!DOCTYPE struts PUBLIC    
  "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
  "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="/pages/rule" namespace="/pages/rule" extends="global">
		<action name="blackRegulation"
			class="cn.grgbanking.feeltm.rule.webapp.BlackRegulationAction">
			<result name="input">/pages/rule/blackRegulation_list.jsp</result>
			<result name="query">/pages/rule/blackRegulation_list.jsp</result>
			<result name="edit">/pages/rule/blackRegulation_edit.jsp</result>
			<result name="add">/pages/rule/blackRegulation_add.jsp</result>
			<result name="show">/pages/rule/blackRegulation_show.jsp</result>
			<result name="info">/pages/rule/blackRegulation_info.jsp</result>
		</action>

		<action name="repeatRegulation"
			class="cn.grgbanking.feeltm.rule.webapp.RepeatRegulationAction">
			<result name="query">/pages/rule/repeatRegulation_list.jsp</result>
			<result name="edit">/pages/rule/repeatRegulation_edit.jsp</result>
			<result name="add">/pages/rule/repeatRegulation_add.jsp</result>
			<result name="show">/pages/rule/repeatRegulation_show.jsp</result>
			<result name="info">/pages/rule/repeatRegulation_info.jsp</result>
		</action>

		<action name="specialRegulation"
			class="cn.grgbanking.feeltm.rule.webapp.SpecialRegulationAction">
			<result name="query">/pages/rule/specialRegulation_list.jsp</result>
			<result name="edit">/pages/rule/specialRegulation_edit.jsp</result>
			<result name="add">/pages/rule/specialRegulation_add.jsp</result>
			<result name="show">/pages/rule/specialRegulation_show.jsp</result>
		</action>

		<action name="regulationDeliver"
			class="cn.grgbanking.feeltm.rule.webapp.RegulationDeliverAction">
			<result name="query">/pages/rule/regulationDeliver_list.jsp</result>
			<result name="show">/pages/rule/regulationDeliver_show.jsp</result>
		</action>
	</package>
</struts>

