<struts>
<package name="/pages/audit"  namespace="/pages/audit" extends="global">
 <action name="auditInfo"  class="cn.grgbanking.feeltm.audit.webapp.AuditInfoAction">
   <result name="query" >/pages/audit/auditInfo_list.jsp</result> 
   <result name="edit" >/pages/audit/auditInfo_edit.jsp</result> 
