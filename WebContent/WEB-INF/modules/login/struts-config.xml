<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>

	<package name="login"   namespace="/" extends="global">
         <action name="login"  class="cn.grgbanking.feeltm.login.webapp.LoginAction" >
			<result name="input">/pages/user_detail.jsp</result> 		
			<result name="successlogin" >/main.jsp</result>		
			<result name="errorlogin" >/index.jsp</result>	
			<result name="headermenu" >/header.jsp</result>	
			<result name="leftmenu" >/left.jsp</result>	
			<result name="mainpage" >/personpage.jsp</result>	
			<result name="personalHome" >/personalHome.jsp</result>	
			<result name="leaderHome" >/leaderHome.jsp</result> 
			<result name="logout" >/pages/common/logout.jsp</result>	
			<result name="success" >/index.jsp</result>
		</action>   
		<action name="LoginLog"  class="cn.grgbanking.feeltm.login.webapp.LoginAction" >
			<result name="list"> /pages/um/loginLogQuery.jsp</result>
		     <result name="watchOnline">/pages/um/loginWatch.jsp</result>   			
		</action>
   </package>
</struts>


